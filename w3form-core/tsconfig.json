{
  "compilerOptions": {
    "composite": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "skipLibCheck": true,
    "emitDeclarationOnly": true,
    "outDir": "dist",
    "lib": [
      "esnext"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "include": ["*.js", "src", "test"],
  "references": [
  ]
}
