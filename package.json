{
  "name": "w3form",
  "type": "module",
  "workspaces": [
    "./w3form-core",
    "./w3form-nodejs",
    "./w3form-workerd"
  ],
  "scripts": {
    "check": "npm run check --if-present --workspaces",
    "dev": "npm run dev --if-present --workspaces",
    "lint": "npm run lint --if-present --workspaces",
    "prepare": "npm run prepare --if-present --workspaces",
    "start": "npm start -w worker-nodejs",
    "test": "npm run test --if-present --workspaces"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "fr32-sha2-256-trunc254-padded-binary-tree-multihash": "3.1.2"
  },
  "overrides": {
    "fr32-sha2-256-trunc254-padded-binary-tree-multihash": "$fr32-sha2-256-trunc254-padded-binary-tree-multihash"
  }
}
