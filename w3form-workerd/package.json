{
  "name": "w3form-workerd",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "npm run build",
    "start": "wrangler pages dev --port=$PORT dist",
    "build": "run-p build:*",
    "dev": "run-p dev:*",
    "deploy": "run-p deploy:*",
    "build:esbuild": "esbuild --bundle ./src/worker.js --format=esm --outfile=dist/_worker.js",
    "build:tsc": "tsc --build",
    "dev:esbuild": "npm run build:esbuild -- --watch",
    "dev:wrangler": "wrangler pages dev dist --live-reload",
    "deploy:wrangler": "wrangler pages publish dist"
  },
  "dependencies": {
    "hono": "^3.12.5",
    "npm-run-all": "^4.1.5",
    "wrangler": "^3.23.0"
  }
}
